        "target": "es5",
        "isolatedModules": false,
        "noImplicitUseStrict": true,
        "strictNullChecks": true,
        "removeComments": true,
        "experimentalDecorators": true,
        "suppressImplicitAnyIndexErrors": true,
        "lib": [
            "es2015",
            "es2017",
            "dom"
