<?xml version="1.0" encoding="UTF-8"?>
<tool type="resizable" height="0" width="100%">
    <presenters> <!-- 描述对 module的引用，其中href为可选，若填写时为私有presenter -->
        <presenter id="GroupLiteracy" />
    </presenters>
    <properties> <!-- 内容模块 key/value 属性 -->
		<!-- independent:是否presenter自主处理,如：常用工具，生字卡 这两类为true -->
        <property name="independent" type="boolean" value="true"/>
        <property name="singleton" type="boolean" value="true"/>
        <!-- execute:执行的接口，如果有配置value执行value里的方法，没有配置执行execute方法 -->
        <property name="execute" type="method" value="execute"/>
		
		<property name="icon" type="image" value="resources/images/icon.png"/>
		<property name="smallIcon" type="image" value="resources/images/smallicon.png"/>
		<property name="toolKey" type="string" value="GroupLiteracy"/>
		<property name="toolName" type="string" value="集体识字"/>
    </properties>
    <modules> <!-- 内容模块实例 -->
        <Module id="main" presenterId="GroupLiteracy" height="100%" width="100%" left="0" top="0">
            <properties>
            </properties>
        </Module>
    </modules>
</tool>