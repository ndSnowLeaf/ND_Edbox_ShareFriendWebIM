<?xml version="1.0" encoding="UTF-8"?>
<tool type="resizable" height="100%" width="100%">
    <presenters>
        <!-- 描述对 module的引用，其中href为可选，若填写时为私有presenter -->
        <presenter id="SubjectToolLauncher" />
    </presenters>
    <properties>
        <!-- 内容模块 key/value 属性 -->
        <property name="independent" type="boolean" value="false"/>
        <property name="singleton" type="boolean" value="true"/>
        <property name=   "icon" type="image" value="resources/images/icon.png"/>
        <property name="smallIcon" type="image" value="resources/images/smallicon.png"/>
        <property name="toolName" type="string" value="弹簧振子"/>
        <property name="execute" type="method" value="execute"/>
        <property name="push" type="boolean" value="true" />
        <property name="enableFull" type="boolean" value="true" />
		<property name="supportXP" type="boolean" value="false" />
    </properties>
    <modules>
        <!-- 内容模块实例 -->
        <Module id="main" presenterId="SubjectToolLauncher" height="100%" width="100%" left="0" top="0">
            <properties>
                <property name="toolName" displayName="弹簧振子" type="string" value="SpringOscillator" isLocalized="false"/>
            </properties>
        </Module>
    </modules>
</tool>